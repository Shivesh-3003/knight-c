# Private key for deployment (DO NOT COMMIT THE ACTUAL .env FILE)
PRIVATE_KEY=your_private_key_here

# Arc Testnet RPC URL
ARC_TESTNET_RPC_URL=https://rpc.arc-testnet.network

# Arc Block Explorer API Key (for contract verification)
ARC_EXPLORER_API_KEY=your_api_key_here

# Circle Gateway API credentials
CIRCLE_API_KEY=your_circle_api_key
CIRCLE_ENTITY_ID=your_circle_entity_id

# Frontend environment variables
VITE_TREASURY_ADDRESS=0x0000000000000000000000000000000000000000
VITE_USDC_ADDRESS=0x0000000000000000000000000000000000000000
VITE_ARC_RPC_URL=https://rpc.arc-testnet.network
VITE_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# Gas reporting
REPORT_GAS=false
